class LetterSymbolDesignation {
  definition {
    A designation realised as a symbol consisting of letters.
  }
  +text: <<BasicDocument>> LocalizedString {
    definition {
      The letter symbol form of the designation.
    }
  }
  +language: Iso639ThreeCharCode {
    definition {
      The language of the designation, as an ISO-639 3-letter code.
    }
  }
  +script: Iso15924Code [0..1] {
    definition {
      The script of the designation, as an ISO-15924 code.
    }
  }
}
