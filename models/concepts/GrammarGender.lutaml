enum GrammarGender {
  definition {
    The grammatical gender of the designation.
  }
  masculine {
    definition {
      Masculine gender.
    }
  }
  feminine {
    definition {
      Feminine gender.
    }
  }
  neuter {
    definition {
      Neuter gender.
    }
  }
  common {
    definition {
      Masculine + Feminine gender.
    }
  }
}
